,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,MAAA,CAAA,WAAA,CAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,CAAA,CAAA;AACA,OAHA,EAGA,GAHA,CAAA;AAIA,KAlCA;AAoCA,IAAA,IApCA,kBAoCA;AACA,UAAA,KAAA,WAAA,GAAA,CAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,KAAA,QAAA,CAAA,KAAA,WAAA,CAAA,EAAA,KAAA,UAAA;AACA,QAAA,KAAA,CACA,GADA,CACA,6CAAA,KAAA,UAAA,CAAA,IAAA,CADA,EACA;AACA,UAAA,IAAA,EAAA,KAAA,UAAA,CAAA,MAAA,CADA;AAEA,UAAA,MAAA,EAAA,KAAA,UAAA,CAAA,QAAA,CAFA;AAGA,UAAA,MAAA,EAAA,KAAA,UAAA,CAAA,QAAA,CAHA;AAIA,UAAA,IAAA,EAAA,KAAA,UAAA,CAAA,MAAA;AAJA,SADA,EAOA,IAPA,CAOA,UAAA,QAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,SATA;AAUA,OAZA,MAaA;AACA,aAAA,QAAA,CAAA,IAAA,CAAA,KAAA,UAAA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,yCAAA,EAAA;AACA,UAAA,IAAA,EAAA,KAAA,UAAA,CAAA,MAAA,CADA;AAEA,UAAA,MAAA,EAAA,KAAA,UAAA,CAAA,QAAA,CAFA;AAGA,UAAA,MAAA,EAAA,KAAA,UAAA,CAAA,QAAA,CAHA;AAIA,UAAA,IAAA,EAAA,KAAA,UAAA,CAAA,MAAA;AAJA,SAAA,EAMA,IANA,CAMA,UAAA,QAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,SARA,EASA,KATA,CASA,UAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,SAXA;AAYA;;AACA,WAAA,KAAA;AACA;AAlEA;AA1DA,CAAA","sourcesContent":["<template>\r\n  <v-data-table\r\n    :headers=\"headers\"\r\n    :items=\"supplier\"\r\n    :search=\"search\"\r\n    class=\"elevation-12 mx-12 mt-12 mb-12 pb-2 pt-2 subtitle-2\"\r\n    dense\r\n    hide-default-footer\r\n  >\r\n    <template v-slot:top>\r\n      <v-toolbar flat color=\"white\" class=\"mt-2 mb-2\">\r\n        <v-toolbar-title>Supplier</v-toolbar-title>\r\n        <v-divider\r\n          class=\"mx-4\"\r\n          inset\r\n          vertical\r\n        ></v-divider>\r\n        <v-icon>mdi-magnify</v-icon>\r\n        <v-text-field\r\n          class=\"pr-12\"\r\n          v-model=\"search\"\r\n          label=\"Search\"\r\n          single-line\r\n          hide-details\r\n        ></v-text-field>\r\n        <v-dialog v-model=\"dialog\" max-width=\"500px\">\r\n          <template v-slot:activator=\"{ on }\">\r\n             <v-btn class=\"mr-12\" fab dark color=\"indigo\" v-on=\"on\">\r\n              <v-icon dark>mdi-plus</v-icon>\r\n            </v-btn>\r\n          </template>\r\n          <v-card>\r\n            <v-card-title>\r\n              <span class=\"headline\">{{ formTitle }}</span>\r\n            </v-card-title>\r\n\r\n            <v-card-text>\r\n              <v-container>\r\n                <v-row>\r\n                  <v-col cols=\"12\" >\r\n                    <v-text-field v-model=\"editedItem.nama\" label=\"Nama Supplier\"></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\">\r\n                    <v-text-field v-model=\"editedItem.alamat\" label=\"Alamat\"></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" >\r\n                    <v-text-field v-model=\"editedItem.noTelp\" label=\"No.Telepon\"></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" >\r\n                    <v-text-field v-model=\"editedItem.kota\" label=\"Kota\"></v-text-field>\r\n                  </v-col>\r\n                </v-row>\r\n              </v-container>\r\n            </v-card-text>\r\n\r\n            <v-card-actions>\r\n              <v-spacer></v-spacer>\r\n              <v-btn color=\"blue darken-1\" text @click=\"close\">Cancel</v-btn>\r\n              <v-btn color=\"blue darken-1\" text @click=\"save\">Save</v-btn>\r\n            </v-card-actions>\r\n          </v-card>\r\n        </v-dialog>\r\n      </v-toolbar>\r\n    </template>\r\n    <template v-slot:item.actions=\"{ item }\">\r\n      <v-icon\r\n        small\r\n        class=\"mr-2\"\r\n        @click=\"editItem(item)\"\r\n      >\r\n        mdi-pencil\r\n      </v-icon>\r\n      <v-icon\r\n        small\r\n        @click=\"deleteItem(item)\"\r\n      >\r\n        mdi-delete\r\n      </v-icon>\r\n    </template>\r\n    <template v-slot:no-data>\r\n      <v-btn color=\"primary\" @click=\"initialize\">Reset</v-btn>\r\n    </template>\r\n  </v-data-table>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data: () => ({\r\n    dialog: false,\r\n    search: '',\r\n    headers: [\r\n      {\r\n        text: 'ID',\r\n        align: 'start',\r\n        sortable: false,\r\n        value: 'id',\r\n        filterable: false \r\n      },\r\n      { text: 'Nama Supplierr', value: 'nama', },\r\n      { text: 'Alamat', value: 'alamat', sortable: false, filterable: false  },\r\n      { text: 'No.Telepon', value: 'noTelp', sortable: false, filterable: false  },\r\n      { text: 'Kota', value: 'kota', filterable: false },\r\n      { text: 'Actions', value: 'actions', sortable: false, filterable: false  },\r\n    ],\r\n    supplier: [],\r\n    tes: '',\r\n    selectedFile: null,\r\n    editedIndex: -1,\r\n    editedItem: {\r\n      nama: '',\r\n      alamat: '',\r\n      noTelp: '',\r\n      kota: '',\r\n    },\r\n    defaultItem: {\r\n      nama: '',\r\n      alamat: '',\r\n      noTelp: '',\r\n      kota: '',\r\n    },\r\n  }),\r\n\r\n  computed: {\r\n    formTitle () {\r\n      return this.editedIndex === -1 ? 'New Item' : 'Edit Item'\r\n    },\r\n  },\r\n\r\n  watch: {\r\n    dialog (val) {\r\n      val || this.close()\r\n    },\r\n  },\r\n\r\n  created () {\r\n    console.log(this.$cookies.get(this.$isLogin.value))\r\n    if(this.$cookies.get(this.$isLogin.value) == 'Login') {\r\n      this.initialize()\r\n    }\r\n    else {\r\n      this.$router.push(\"/menu/login\")\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    initialize () {\r\n      axios.get(\"http://luxinoire.com/api/showSupplier\").then(response => {\r\n        this.supplier = response.data;\r\n        this.index = response.data.length\r\n        console.log(this.supplier)\r\n      });\r\n      this.$adminDrawer.value = true\r\n      console.log(this.$adminDrawer.value)\r\n    },\r\n\r\n    editItem (item) {\r\n      this.editedIndex = this.supplier.indexOf(item)\r\n      this.editedItem = Object.assign({}, item)\r\n      this.dialog = true\r\n    },\r\n\r\n    deleteItem (item) {\r\n      const index = this.supplier.indexOf(item)\r\n      var temp = Object.assign({}, item)\r\n      console.log(temp[\"id\"])\r\n      confirm('Are you sure you want to delete this item?') && this.supplier.splice(index, 1) &&\r\n      axios.delete(\"http://luxinoire.com/api/deleteSupplier/\"+ temp[\"id\"])\r\n      .then(response => {\r\n        console.log(response)\r\n      });\r\n    },\r\n\r\n    close () {\r\n      this.dialog = false\r\n      setTimeout(() => {\r\n        this.editedItem = Object.assign({}, this.defaultItem)\r\n        this.editedIndex = -1\r\n      }, 300)\r\n    },\r\n\r\n    save () {\r\n      if (this.editedIndex > -1) {\r\n        Object.assign(this.supplier[this.editedIndex], this.editedItem)\r\n        axios\r\n        .put(\"http://luxinoire.com/api/updateSupplier/\"+this.editedItem[\"id\"], {\r\n          nama: this.editedItem[\"nama\"],\r\n          alamat: this.editedItem[\"alamat\"],\r\n          noTelp: this.editedItem[\"noTelp\"],\r\n          kota: this.editedItem[\"kota\"],\r\n        })\r\n        .then(response => {\r\n          console.log(response.data)\r\n        });\r\n      } \r\n      else {\r\n        this.supplier.push(this.editedItem)\r\n        axios.post(\"http://luxinoire.com/api/createSupplier\", {\r\n                nama: this.editedItem[\"nama\"],\r\n                alamat: this.editedItem[\"alamat\"],\r\n                noTelp: this.editedItem[\"noTelp\"],\r\n                kota: this.editedItem[\"kota\"],\r\n          })\r\n          .then(response => {\r\n            console.log(response.data)\r\n          })\r\n          .catch(error => {\r\n            console.log(error)  \r\n          })\r\n      }\r\n      this.close()\r\n    },\r\n  },\r\n}\r\n</script>"],"sourceRoot":"src/views"}]}